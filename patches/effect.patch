+        }
+      })
+    } else if ("scoped" in maker) {
+      Object.defineProperty(TagClass, layerName, {
+        get(this: any) {
+          return layerCache ??= layer.scoped(TagClass, map(maker.scoped, (_) => new this(_)))
+        }
+      })
+    } else if ("sync" in maker) {
+      Object.defineProperty(TagClass, layerName, {
