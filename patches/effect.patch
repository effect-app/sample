+      Object.defineProperty(TagClass, "Default", {
+        get(this: any) {
+          return layerWithDepsCache ??= layer.provide(
+            this.DefaultWithoutDependencies,
+            maker.dependencies
+          )
+        }
+      })
+    }
+
+    return proxy === true ? makeTagProxy(TagClass) : TagClass
+  }
