{
  "name": "@effect-app-boilerplate/api",
  "version": "0.0.1",
  "main": "./dist/main.js",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "circular": "pnpm circular:src && pnpm circular:dist",
    "circular:src": "madge --circular --ts-config ./tsconfig.json --extensions ts ./src",
    "circular:dist": "madge --circular --extensions js ./dist",
    "clean": "rm -rf dist test/dist",
    "clean-dist": "sh ../scripts/clean-dist.sh",
    "build": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=8192 tspc --build",
    "watch": "pnpm build --watch",
    "watch2": "pnpm clean-dist && NODE_OPTIONS=--max-old-space-size=8192 tspc -w",
    "compile": "NODE_OPTIONS=--max-old-space-size=8192 tspc --noEmit",
    "lint": "NODE_OPTIONS=--max-old-space-size=8192 ESLINT_TS=1 eslint src test",
    "lint:watch": "ESLINT_TS=1 esw -w --changed --clear --ext ts,tsx src test",
    "autofix": "pnpm lint --fix",
    "test": "vitest",
    "test:run": "pnpm run test run --passWithNoTests",
    "testsuite": "pnpm circular && pnpm run test:run && pnpm lint",
    "dev": "NODE_ENV=development nodemon --signal SIGTERM --exec pnpm dev:tsx",
    "dev:compiled": "pnpm start:compiled",
    "dev:tsx": "pnpm start:tsx",
    "dev:vite": "vite-node --watch ./src/main.ts",
    "start": "pnpm start:compiled",
    "start:compiled": "node -r source-map-support/register ./dist/main.js",
    "start:tsx": "tsx ./src/main.ts",
    "ncu": "ncu",
    "extract:i18n": "formatjs extract './**/*.ts' --ignore './**/*.d.ts' --format src/i18n/extraction-formatter.cjs --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file src/i18n/extracted/en.json"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./_cjs/index.cjs"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      },
      "require": {
        "types": "./dist/*.d.ts",
        "default": "./_cjs/*.cjs"
      }
    }
  },
  "dependencies": {
    "@azure/arm-monitor": "^7.0.0",
    "@azure/cosmos": "^4.1.1",
    "@azure/service-bus": "^7.9.5",
    "@azure/storage-blob": "^12.25.0",
    "@effect-app/core": "^1.17.1",
    "@effect-app/infra": "1.43.4",
    "@effect-app/infra-adapters": "^1.20.4",
    "@effect-app/schema": "^1.19.1",
    "effect-app": "^1.30.3",
    "@effect/platform": "^0.68.6",
    "@effect/opentelemetry": "^0.38.2",
    "@effect/platform-node": "0.63.6",
    "@effect/rpc": "^0.43.5",
    "@effect/rpc-http": "^0.41.5",
    "@effect/vitest": "^0.12.1",
    "@formatjs/cli": "^6.2.15",
    "@formatjs/intl": "2.10.8",
    "@mollie/api-client": "^4.1.0",
    "@opentelemetry/auto-instrumentations-node": "^0.51.0",
    "@opentelemetry/context-async-hooks": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@sendgrid/mail": "^8.1.4",
    "@sentry/node": "^8.2.1",
    "@sentry/opentelemetry": "^8.2.1",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "cross-fetch": "^4.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "effect": "^3.9.2",
    "express": "^4.21.1",
    "express-compression": "^1.0.2",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "fast-check": "^3.22.0",
    "jwks-rsa": "2.1.4",
    "jwt-decode": "^4.0.0",
    "object-hash": "^3.0.0",
    "papaparse": "^5.4.1",
    "redis": "^3.1.2",
    "redlock": "^5.0.0-beta.2",
    "redoc": "^2.2.0",
    "redoc-express": "^2.1.0",
    "source-map-support": "^0.5.21",
    "stopwatch-node": "^1.1.0",
    "swagger-ui-express": "^5.0.1",
    "tcp-port-used": "^1.0.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/redis": "^2.8.32",
    "@types/swagger-ui-express": "^4.1.6",
    "eslint-plugin-formatjs": "^5.1.0",
    "typescript": "^5.6.3"
  }
}